@model IEnumerable<DLLGakuen.Entity.OrderList>

@{
    ViewBag.Title = "OrderListList";
}

<h2>OrderListList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PaidStringCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PriceToPay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PaidStringCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PriceToPay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateTime)
        </td>
        <td>
            @Html.ActionLink("Details", "ListInfo", new { id=item.Id }) |
        </td>
    </tr>
}

</table>
